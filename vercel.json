{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "env": {},
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "^/(?!.*\\.).*$",
      "dest": "/index.html"
    },
    {
      "src": "^/static/(?P<file>.*)$",
      "dest": "/static/$file"
    }
  ]
}
